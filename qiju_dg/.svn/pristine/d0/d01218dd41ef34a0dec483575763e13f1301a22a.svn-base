<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Titletext">
        <item name="android:background">@drawable/round_background</item>
        <item name="android:hint">@string/app_name</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="main_text">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/text_font_size12</item>
    </style>

    <style name="main_list_text">
        <item name="android:textSize">@dimen/text_font_size12</item>
        <item name="android:lineSpacingExtra">@dimen/text_linespacing</item>
    </style>

    <style name="AppTheme.NoActionBar">

    </style>

    <!--标题-->
    <style name="title_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/title_font_size17</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <!--line-->
    <style name="line_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:background">@color/gray_line</item>
    </style>

    <!-- popup window 显示消失动画-->
    <style name="PopWindowAnimStyle">
        <item name="android:windowShowAnimation">@anim/dialog_enter</item>
        <!-- 指定显示的动画xml  -->
        <item name="android:windowHideAnimation">@anim/dialog_exit</item>
        <!-- 指定消失的动画xml  -->
    </style>

    <!-- my loading dialog -->
    <style name="MyDialogStyle1">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="take_photo_anim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/pop_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/pop_exit_anim</item>
    </style>
    <!--EditText Hint 字体颜色-->
    <style name="et_hint">
        <item name="android:textColorHint">@color/text_gray</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="style_navigation_tab_ll">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">#fff</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>

    </style>

    <style name="style_navigation_tab_iv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="style_navigation_tab_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_marginTop">3dp</item>
    </style>

    <!--fragment_second-->
    <style name="second_huizong_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>

    <style name="second_num_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>

    <style name="second_time_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>

    <style name="second_bottom_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>

    <!--fragment_third-->
    <style name="third_theme_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>

    <style name="third_time_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#abc</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>

    <style name="third_money_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#f30</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_marginTop">3dp</item>
    </style>

    <style name="customer_detail_textView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black_text</item>
        <item name="android:textSize">@dimen/text_font_size12</item>
    </style>

    <style name="customer_detail_editText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:lines">1</item>
        <item name="android:textColor">@color/black_text</item>
        <item name="android:textSize">@dimen/text_font_size12</item>
        <item name="android:textColorHint">@color/text_gray</item>
        <item name="android:background">@null</item>
    </style>

    <style name="customer_money_editText">
        <item name="android:layout_height">match_parent</item>
        <item name="android:lines">2</item>
        <item name="android:textSize">@dimen/text_font_size12</item>
        <item name="android:textColorHint">@color/text_gray</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:background">@null</item>
    </style>

    <style name="width_weight">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="width_weight.evaluate_list_text">
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/text_font_size12</item>
        <item name="android:textColor">@color/text_gray_2</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="statement">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:padding">10sp</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>

    <style name="notify_text_top">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>
</resources>
